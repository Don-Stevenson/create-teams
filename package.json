{
  "name": "team-creator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "server": "node server/server.js",
    "lint": "next lint",
    "test:FE": "jest --updateSnapshot",
    "test:FE:watch": "jest --watch --updateSnapshot",
    "test:FE:coverage": "npm run test:FE -- --coverage --updateSnapshot",
    "test:BE": "cd server && node --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles",
    "test:BE:watch": "cd server && node --experimental-vm-modules node_modules/jest/bin/jest.js --watch --detectOpenHandles",
    "test:BE:coverage": "cd server && node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --detectOpenHandles"
  },
  "dependencies": {
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.7",
    "jest": "^29.7.0",
    "next": "^14.2.23",
    "react": "^18",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react-beautiful-dnd": "^13.1.8",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1"
  }
}
